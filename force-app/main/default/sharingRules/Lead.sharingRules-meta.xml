            <guestUser>Autocita_Arriaga</guestUser>
        </sharedTo>
        <criteriaItems>
            <field>LastName</field>
            <operation>notEqual</operation>
            <value></value>
        </criteriaItems>
        <includeHVUOwnedRecords>false</includeHVUOwnedRecords>
    </sharingGuestRules>
</SharingRules>
